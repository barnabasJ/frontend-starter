{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "pnpm clean && concurrently \"pnpm build:server\" \"wait-on dist/server/index.cjs && pnpm start:server --watch dist/server\"",
    "clean": "rimraf dist",
    "build": "pnpm clean && pnpm build:server && pnpm build:client",
    "build:server": "node build.js",
    "build:client": "vite build --outDir dist/client",
    "build:ssr": "vite build --outDir dist/server --ssr src/ssr-entry.tsx --target node18",
    "start:server": "nodemon dist/server/index.cjs",
    "preview": "vite preview",
    "graphql-codegen": "graphql-codegen --config codegen.yml",
    "watch-codegen": "graphql-codegen --config codegen.yml --watch 'src/**/*.graphql'"
  },
  "dependencies": {
    "@fastify/express": "^2.3.0",
    "@fastify/http-proxy": "^8.4.0",
    "@fastify/middie": "^8.1.0",
    "@fastify/static": "^6.6.0",
    "@reduxjs/toolkit": "^1.9.1",
    "@rtk-query/graphql-request-base-query": "^2.2.0",
    "@types/node": "^18.11.18",
    "esbuild": "^0.16.14",
    "fastify": "^4.11.0",
    "fastify-plugin": "^4.4.0",
    "graphql": "^16.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.6.1"
  },
  "devDependencies": {
    "@esbuild-plugins/tsconfig-paths": "^0.0.4",
    "@graphql-codegen/add": "^3.2.3",
    "@graphql-codegen/cli": "2.16.3",
    "@graphql-codegen/introspection": "^2.2.3",
    "@graphql-codegen/near-operation-file-preset": "^2.4.4",
    "@graphql-codegen/typed-document-node": "^2.3.12",
    "@graphql-codegen/typescript": "2.8.7",
    "@graphql-codegen/typescript-operations": "^2.5.12",
    "@graphql-codegen/typescript-rtk-query": "^2.3.7",
    "@graphql-typed-document-node/core": "^3.1.0",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@vitejs/plugin-react": "^3.0.1",
    "concurrently": "^7.6.0",
    "esbuild-plugin-path-alias": "^1.0.7",
    "nodemon": "^2.0.20",
    "pino-pretty": "^9.1.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.9.3",
    "vite": "^4.0.4",
    "vite-tsconfig-paths": "^4.0.3",
    "wait-on": "^7.0.1"
  }
}
